-module(test_t).

%% API
-export([start/0]).

%%%

%% --------------------------------------------------------------------
%% Include files
%% --------------------------------------------------------------------
-include("log.api").
-define(Appl,t).

%%%===================================================================
%%% API
%%%===================================================================

%%--------------------------------------------------------------------
%% @doc
%% 
%% @end
%%--------------------------------------------------------------------
start()->
    io:format("Start ~p~n",[{?MODULE,?FUNCTION_NAME}]),
    ok=test_template(),
    ok.


%%--------------------------------------------------------------------
%% @doc
%% test of template 
%% @end
%%--------------------------------------------------------------------
test_template()->
    io:format("Start ~p~n",[{?MODULE,?FUNCTION_NAME,?LINE}]),

    pong=?Appl:ping(),
    {Y,M,D}=date(),
    {ok,Y,M,D}=?Appl:template_call(args),
    ok=?Appl:template_cast(args),
    
    ok.


    

